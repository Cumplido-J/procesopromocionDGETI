<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Servlet_cancelarPin</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_cancelarPin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_cbRegistroCriterios</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_cbRegistroCriterios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_registrarCriterio</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_registrarCriterio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_registrarCurso</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_registrarCurso</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet_cancelarPin</servlet-name>
        <url-pattern>/Servlet_cancelarPin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_cbRegistroCriterios</servlet-name>
        <url-pattern>/Servlet_cbRegistroCriterios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_registrarCriterio</servlet-name>
        <url-pattern>/Servlet_registrarCriterio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_registrarCurso</servlet-name>
        <url-pattern>/Servlet_registrarCurso</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
