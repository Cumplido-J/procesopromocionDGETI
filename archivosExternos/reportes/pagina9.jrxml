		<parameter name="plantel" class="java.lang.Integer"/>
		<parameter name="convocatoria" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT vacancia.idCategoria as tipocategoria,
	vacancia.plazas as cantidadplazas,
	CASE
		WHEN vacancia.horas is not null then concat(catcategoriasplaza.categoria,"-",catjornada.jornada,"(",vacancia.horas,")","-",vacancia.tipoVacancia)
		ELSE concat(catcategoriasplaza.categoria,"-",catjornada.jornada,"-",vacancia.tipoVacancia)
	END as categoria
FROM vacancia
	inner join catcategoriasplaza ON 
	 vacancia.idCategoria = catcategoriasplaza.id 
    inner join catjornada on
     vacancia.idJornada=catjornada.id
	INNER JOIN convocatoria conv
	ON conv.idPlantel=vacancia.idPlantel and conv.idPrograma=vacancia.idPrograma
WHERE 
	 conv.id =  $P{convocatoria}
	 AND vacancia.idPlantel =  $P{plantel} 
ORDER BY catcategoriasplaza.categoria DESC, catjornada.id desc,vacancia.tipoVacancia DESC]]>
		</queryString>
