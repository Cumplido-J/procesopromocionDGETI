        <servlet-class>com.aplicacion.servlet.Servlet_consultaCatalogos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_guardarArchivo</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_guardarArchivo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_muestraArchivo</servlet-name>
        <servlet-class>com.aplicacion.servlet.Servlet_muestraArchivo</servlet-class>
