        <servlet-class>com.aplicacion.servlet.Servlet_registroInfoCompatibilidad</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet_consultaCatalogos</servlet-name>
        <url-pattern>/Servlet_consultaCatalogos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_guardarArchivo</servlet-name>
        <url-pattern>/Servlet_guardarArchivo</url-pattern>
